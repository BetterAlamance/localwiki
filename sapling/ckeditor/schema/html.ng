<element name="html" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <element name="head">
    <element name="title"><text/></element>
  </element>
  <element name="body">
    <element name="a">
      <attribute name="href">
        <data type="anyURI">
          <param name="pattern">http://[a-zA-Z0-9\.]+</param>
        </data>
      </attribute>
      <text/>
    </element>
  </element>
</element>